.page-index {
  .style-list {
    list-style: none;

    > .item {
      float: left;
      padding: 30px;

      /**
       * 描述
       */
      .desc {
        margin-top: 20px;
        color: #999;
        font-size: 12px;
        padding: 10px;
        border-top: 1px #f2f2f2 dotted;
      }

      /**
       * 兼容性列表
       */
      .compatibility {
        margin-left: 0;
        padding-left: 0;
      }
    }
  }
}